{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard", "plugin:jest/recommended"],
  "overrides": [],
  "settings": {
    "react": {
      "version": "18.2.0"
    }
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "jest"],
  "rules": {
    "space-before-function-paren": 0
  }
}
